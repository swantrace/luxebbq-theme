// product page //
.product-slick, .rtl-product-slick, .product-right-slick, .rtl-product-right-slick {
  .slick-prev, .slick-next {
    margin-top: -10px;
    &:before {
      font: normal normal normal 14px/1 $font-awesome;
      opacity: 1;
      color: black;
      background-color: white;
      border-radius: 100%;
      height: 100%;
      display: flex;
      justify-content: center;
      align-items: center;
    }
  }
  .slick-next {
    right:20px;
    z-index: 1;
    &:before {
      content: "\f105";
    }
  }
  .slick-prev {
    left:20px;
    z-index: 1;
    &:before {
      content: "\f104";
    }
  }
  .slick-slide {
    > div {
      display: flex;
      margin-bottom: 15px;
      img{
        margin: 0 auto;
      }
    }
  }
}
.slider-nav, .rtl-slider-nav, .slider-right-nav, .rtl-slider-right-nav {
  .slick-slide {
    background: transparent;
    img{
      margin: 0 auto;
    }
    > div {
      display: flex;
      margin: 15px 15px 0 15px;
      border: 1px solid $round-border;
      transition: all 0.5s ease;
    }
    &.slick-current {
      > div {
        border-color: $font-grey;
        transition: all 0.5s ease;
      }
    }
    &.slick-active {
      transition: all 0.5s ease;
    }
  }
  /*   .slick-track{
  transform: translate3d(0, 0, 0) !important;
} */
}
.slider-right-nav, .rtl-slider-right-nav {
  .slick-slide {
    &:first-child {
      > div {
        margin-top: 0;
      }
    }
  }
}
