/*=====================
02.Button CSS start
==========================*/
button {
  cursor: pointer;
}
.btn{
  line-height: 20px;
  text-transform: uppercase;
  font-size: 14px;
  font-weight: 700;
  border-radius: 0;
  transition: .3s ease-in-out;
  &:hover {
    transition: .3s ease-in-out;
  }
  &:focus{
    box-shadow: none;
  }
}
.btn-solid {
  padding:13px 29px;
  color: {{ settings.primary_button_text_color }};
  letter-spacing: 0.05em;
  border: 2px solid {{ settings.primary_button_border_color }};
  background-color:  {{ settings.primary_button_bg_color }};
  background-size: 540px;
  background-repeat: no-repeat;
  background-position: 0;
  -webkit-transition: background 300ms ease-in-out;
  transition: background 300ms ease-in-out;
  &:hover{
    background-position: 100%;
    color: {{ settings.primary_button_text_color_hover }};
    background-color: {{ settings.primary_button_bg_color_hover }};
  }
  &.black-btn {
    background-color:  {{ settings.primary_button_bg_color }};
    border: 2px solid {{ settings.primary_button_border_color }};
  }
  &.btn-gradient {
    background: $theme-deafult;
    background: linear-gradient(180deg, $gradiant-primary 0%, $gradiant-seconadary 99%);
    transition: background 300ms ease-in-out;
    background-size: 300% 100%;
    border:none;
    &:hover {
      background: linear-gradient(-180deg, $gradiant-seconadary 0%, $gradiant-primary 99%);
      transition: background 300ms ease-in-out;
      color: white;
    }
  }
  &.btn-green {
    background-image: linear-gradient(to right, $gradiant-primary, $gradiant-seconadary);
    border:none;
    background-color: $theme-deafult;
    &:hover {
      background-color: $theme-deafult;
      background-image: none;
      color: white;
    }
  }
  &.btn-sm {
    padding:9px 16px;
  }
  &.btn-xs {
    padding: 5px 8px;
    text-transform: capitalize;
  }
}
.btn-outline {
  display:inline-block;
  padding:13px 29px;
  letter-spacing: 0.05em;
  border:2px solid {{ settings.secondary_button_border_color }};
  position:relative;
  color: {{ settings.secondary_button_text_color }};
  &:before {
    -webkit-transition:0.5s all ease;
    transition:0.5s all ease;
    position:absolute;
    top:0;
    left:50%;
    right:50%;
    bottom:0;
    opacity:0;
    content:'';
    background-color:{{ settings.secondary_button_bg_color }};
    z-index:-2;
  }
  &:hover {
    color: {{ settings.secondary_button_text_color_hover }};
    &:before {
      -webkit-transition:0.5s all ease;
      transition:0.5s all ease;
      left:0;
      right:0;
      opacity:1;
    }
  }
  &:focus {
    &:before {
      transition:0.5s all ease;
      left:0;
      right:0;
      opacity:1;
    }
  }
}
.btn-classic {
  &:hover {
    background-color: $theme-deafult;
  }
}
.btn-theme {
  background-color: $theme-deafult;
  color: $white;
}
button {
  &.btn {
    &.btn-solid, &.btn-outline {
      &:active {
        background-image: -webkit-linear-gradient(30deg, {{ settings.primary_button_bg_color }} 50%, transparent 50%);
        background-image: linear-gradient(30deg, {{ settings.primary_button_bg_color }} 50%, transparent 50%);
        color: {{ settings.primary_button_text_color }};
        background: {{ settings.primary_button_bg_color }};
      }
    }
  }
}
button[disabled]{cursor:not-allowed;}

