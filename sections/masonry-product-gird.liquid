                    {% assign block_image = product1 %}        
                    {% assign img_url = block_image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
                    <img src="{{ block_image | img_url: 'original' }}" 
                         data-src="{{ img_url }}" 
                         data-widths="[157, 270, 303, 370, 377, 670, 570, 720, 900, 1080, 1296, 1512, 1728, 2048]" 
                         data-aspectratio="{{ block_image.aspect_ratio }}" 
                         data-sizes="auto" 
                         class="lazyload {% if settings.enable_blur_lazy_load %} blur-up {% endif %} img-fluid" alt="{{ product1.title | split: '|' | first }}" >
                  </a>
                  {% endif %}
                </div>
                <div class="cart-info cart-wrap">
                  {% if settings.show_catelogue != true %}
                  <form method="post" enctype="multipart/form-data" action="/cart/add" class="product_grid_cart_form" id="product_grid_id_{{ product1.variants.first.id }}">
                    <input type="hidden" name="id" value="{{ product1.variants.first.id }}" />
                    <button  data-toggle="tooltip" data-placement="left" title="Add to cart" class="add_to_cart_btn_cls fly_addtocart" {% unless product1.variants[0].available%}disabled{% endunless %}>
                      <i class="ti-shopping-cart" ></i>
                    </button>
