{% include 'breadcrumb' %}

<!-- section start -->
<section class="p-0">
  <div class="container">
    <div class="row">
      <div class="col-sm-12">
        <div class="error-section">
          <h1 {% if settings.enable_lang_switcher %}data-trans-key="general.404.title"{%endif%}>{{ 'general.404.title' | t }}</h1>
          <h2 {% if settings.enable_lang_switcher %}data-trans-key="general.404.subtext"{%endif%}>{{ 'general.404.subtext' | t }}</h2>
          <a href="/" class="btn btn-solid" {% if settings.enable_lang_switcher %}data-trans-key="general.404.link"{%endif%}>{{ 'general.404.link' | t }}</a>
        </div>
      </div>
    </div>
  </div>
</section>
<!-- Section ends -->